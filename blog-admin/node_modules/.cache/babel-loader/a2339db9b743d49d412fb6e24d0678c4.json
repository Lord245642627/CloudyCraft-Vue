{"ast":null,"code":"export default {\n  created() {\n    this.current = this.$store.state.pageState.tag;\n    this.listTags();\n  },\n\n  data: function () {\n    return {\n      isDelete: false,\n      loading: true,\n      addOrEdit: false,\n      keywords: null,\n      tags: [],\n      tagIds: [],\n      tagForm: {\n        id: null,\n        tagName: '',\n        sort: ''\n      },\n      current: 1,\n      size: 10,\n      count: 0\n    };\n  },\n  methods: {\n    selectionChange(tags) {\n      this.tagIds = [];\n      tags.forEach(item => {\n        this.tagIds.push(item.id);\n      });\n    },\n\n    searchTags() {\n      this.current = 1;\n      this.listTags();\n    },\n\n    sizeChange(size) {\n      this.size = size;\n      this.listTags();\n    },\n\n    currentChange(current) {\n      this.current = current;\n      this.$store.commit('updateTagPageState', current);\n      this.listTags();\n    },\n\n    deleteTag(id) {\n      var param = {};\n\n      if (id == null) {\n        param = {\n          data: this.tagIds\n        };\n      } else {\n        param = {\n          data: [id]\n        };\n      }\n\n      this.axios.delete('/api/admin/tags', param).then(({\n        data\n      }) => {\n        if (data.code === 0) {\n          this.$notify.success({\n            title: '成功',\n            message: data.message\n          });\n          this.listTags();\n        } else {\n          this.$notify.error({\n            title: '失败',\n            message: data.message\n          });\n        }\n      });\n      this.isDelete = false;\n    },\n\n    listTags() {\n      this.axios.get('/api/admin/tags', {\n        params: {\n          current: this.current,\n          size: this.size,\n          keywords: this.keywords\n        }\n      }).then(({\n        data\n      }) => {\n        this.tags = data.data.records;\n        this.count = data.data.count;\n        this.loading = false;\n      });\n    },\n\n    openModel(tag) {\n      if (tag != null) {\n        this.tagForm = JSON.parse(JSON.stringify(tag));\n        this.$refs.tagTitle.innerHTML = '修改标签';\n      } else {\n        this.tagForm.id = null;\n        this.tagForm.tagName = '';\n        this.$refs.tagTitle.innerHTML = '添加标签';\n      }\n\n      this.addOrEdit = true;\n    },\n\n    addOrEditTag() {\n      if (this.tagForm.tagName.trim() == '') {\n        this.$message.error('标签名不能为空');\n        return false;\n      }\n\n      this.axios.post('/api/admin/tags', this.tagForm).then(({\n        data\n      }) => {\n        if (data.code === 0) {\n          this.$notify.success({\n            title: '成功',\n            message: data.message\n          });\n          this.listTags();\n        } else {\n          this.$notify.error({\n            title: '失败',\n            message: data.message\n          });\n        }\n      });\n      this.addOrEdit = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsFA;EACAA;IACA;IACA;EACA,CAJA;;EAKAC;IACA;MACAC,eADA;MAEAC,aAFA;MAGAC,gBAHA;MAIAC,cAJA;MAKAC,QALA;MAMAC,UANA;MAOAC;QACAC,QADA;QAEAC,WAFA;QAGAC;MAHA,CAPA;MAYAC,UAZA;MAaAC,QAbA;MAcAC;IAdA;EAgBA,CAtBA;EAuBAC;IACAC;MACA;MACAV;QACA;MACA,CAFA;IAGA,CANA;;IAOAW;MACA;MACA;IACA,CAVA;;IAWAC;MACA;MACA;IACA,CAdA;;IAeAC;MACA;MACA;MACA;IACA,CAnBA;;IAoBAC;MACA;;MACA;QACAC;UAAApB;QAAA;MACA,CAFA,MAEA;QACAoB;UAAApB;QAAA;MACA;;MACA;QAAAA;MAAA;QACA;UACA;YACAqB,WADA;YAEAC;UAFA;UAIA;QACA,CANA,MAMA;UACA;YACAD,WADA;YAEAC;UAFA;QAIA;MACA,CAbA;MAcA;IACA,CA1CA;;IA2CAC;MACA,WACAC,GADA,CACA,iBADA,EACA;QACAC;UACAd,qBADA;UAEAC,eAFA;UAGAR;QAHA;MADA,CADA,EAQAsB,IARA,CAQA;QAAA1B;MAAA;QACA;QACA;QACA;MACA,CAZA;IAaA,CAzDA;;IA0DA2B;MACA;QACA;QACA;MACA,CAHA,MAGA;QACA;QACA;QACA;MACA;;MACA;IACA,CApEA;;IAqEAC;MACA;QACA;QACA;MACA;;MACA;QAAA5B;MAAA;QACA;UACA;YACAqB,WADA;YAEAC;UAFA;UAIA;QACA,CANA,MAMA;UACA;YACAD,WADA;YAEAC;UAFA;QAIA;MACA,CAbA;MAcA;IACA;;EAzFA;AAvBA","names":["created","data","isDelete","loading","addOrEdit","keywords","tags","tagIds","tagForm","id","tagName","sort","current","size","count","methods","selectionChange","searchTags","sizeChange","currentChange","deleteTag","param","title","message","listTags","get","params","then","openModel","addOrEditTag"],"sourceRoot":"src/views/tag","sources":["Tag.vue"],"sourcesContent":["<template>\n  <el-card class=\"main-card\">\n    <div class=\"title\">{{ this.$route.name }}</div>\n    <div class=\"operation-container\">\n      <el-button type=\"primary\" size=\"small\" icon=\"el-icon-plus\" @click=\"openModel(null)\"> 新增 </el-button>\n      <el-button\n        type=\"danger\"\n        size=\"small\"\n        icon=\"el-icon-delete\"\n        :disabled=\"tagIds.length == 0\"\n        @click=\"isDelete = true\">\n        批量删除\n      </el-button>\n      <div style=\"margin-left: auto\">\n        <el-input\n          v-model=\"keywords\"\n          prefix-icon=\"el-icon-search\"\n          size=\"small\"\n          placeholder=\"请输入标签名\"\n          style=\"width: 200px\"\n          @keyup.enter.native=\"searchTags\" />\n        <el-button type=\"primary\" size=\"small\" icon=\"el-icon-search\" style=\"margin-left: 1rem\" @click=\"searchTags\">\n          搜索\n        </el-button>\n      </div>\n    </div>\n    <el-table border :data=\"tags\" v-loading=\"loading\" @selection-change=\"selectionChange\">\n      <el-table-column type=\"selection\" width=\"55\" />\n      <el-table-column prop=\"tagName\" label=\"标签名\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-tag>\n            {{ scope.row.tagName }}\n          </el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"articleCount\" label=\"文章量\" align=\"center\" />\n      <el-table-column prop=\"sort\" label=\"序号\" align=\"center\" />\n      <el-table-column prop=\"createTime\" label=\"创建时间\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <i class=\"el-icon-time\" style=\"margin-right: 5px\" />\n          {{ scope.row.createTime | date }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" width=\"160\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" size=\"mini\" @click=\"openModel(scope.row)\"> 编辑 </el-button>\n          <el-popconfirm title=\"确定删除吗？\" style=\"margin-left: 1rem\" @confirm=\"deleteTag(scope.row.id)\">\n            <el-button size=\"mini\" type=\"danger\" slot=\"reference\"> 删除 </el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <el-pagination\n      class=\"pagination-container\"\n      background\n      @size-change=\"sizeChange\"\n      @current-change=\"currentChange\"\n      :current-page=\"current\"\n      :page-size=\"size\"\n      :total=\"count\"\n      :page-sizes=\"[10, 20]\"\n      layout=\"total, sizes, prev, pager, next, jumper\" />\n    <el-dialog :visible.sync=\"isDelete\" width=\"30%\">\n      <div class=\"dialog-title-container\" slot=\"title\"><i class=\"el-icon-warning\" style=\"color: #ff9900\" />提示</div>\n      <div style=\"font-size: 1rem\">是否删除选中项？</div>\n      <div slot=\"footer\">\n        <el-button @click=\"isDelete = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"deleteTag(null)\"> 确 定 </el-button>\n      </div>\n    </el-dialog>\n    <el-dialog :visible.sync=\"addOrEdit\" width=\"30%\">\n      <div class=\"dialog-title-container\" slot=\"title\" ref=\"tagTitle\" />\n      <el-form label-width=\"80px\" size=\"medium\" :model=\"tagForm\">\n        <el-form-item label=\"标签名\">\n          <el-input style=\"width: 220px\" v-model=\"tagForm.tagName\" />\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\">\n        <el-button @click=\"addOrEdit = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"addOrEditTag\"> 确 定 </el-button>\n      </div>\n    </el-dialog>\n  </el-card>\n</template>\n\n<script>\nexport default {\n  created() {\n    this.current = this.$store.state.pageState.tag\n    this.listTags()\n  },\n  data: function () {\n    return {\n      isDelete: false,\n      loading: true,\n      addOrEdit: false,\n      keywords: null,\n      tags: [],\n      tagIds: [],\n      tagForm: {\n        id: null,\n        tagName: '',\n        sort: ''\n      },\n      current: 1,\n      size: 10,\n      count: 0\n    }\n  },\n  methods: {\n    selectionChange(tags) {\n      this.tagIds = []\n      tags.forEach((item) => {\n        this.tagIds.push(item.id)\n      })\n    },\n    searchTags() {\n      this.current = 1\n      this.listTags()\n    },\n    sizeChange(size) {\n      this.size = size\n      this.listTags()\n    },\n    currentChange(current) {\n      this.current = current\n      this.$store.commit('updateTagPageState', current)\n      this.listTags()\n    },\n    deleteTag(id) {\n      var param = {}\n      if (id == null) {\n        param = { data: this.tagIds }\n      } else {\n        param = { data: [id] }\n      }\n      this.axios.delete('/api/admin/tags', param).then(({ data }) => {\n        if (data.code === 0) {\n          this.$notify.success({\n            title: '成功',\n            message: data.message\n          })\n          this.listTags()\n        } else {\n          this.$notify.error({\n            title: '失败',\n            message: data.message\n          })\n        }\n      })\n      this.isDelete = false\n    },\n    listTags() {\n      this.axios\n        .get('/api/admin/tags', {\n          params: {\n            current: this.current,\n            size: this.size,\n            keywords: this.keywords\n          }\n        })\n        .then(({ data }) => {\n          this.tags = data.data.records\n          this.count = data.data.count\n          this.loading = false\n        })\n    },\n    openModel(tag) {\n      if (tag != null) {\n        this.tagForm = JSON.parse(JSON.stringify(tag))\n        this.$refs.tagTitle.innerHTML = '修改标签'\n      } else {\n        this.tagForm.id = null\n        this.tagForm.tagName = ''\n        this.$refs.tagTitle.innerHTML = '添加标签'\n      }\n      this.addOrEdit = true\n    },\n    addOrEditTag() {\n      if (this.tagForm.tagName.trim() == '') {\n        this.$message.error('标签名不能为空')\n        return false\n      }\n      this.axios.post('/api/admin/tags', this.tagForm).then(({ data }) => {\n        if (data.code === 0) {\n          this.$notify.success({\n            title: '成功',\n            message: data.message\n          })\n          this.listTags()\n        } else {\n          this.$notify.error({\n            title: '失败',\n            message: data.message\n          })\n        }\n      })\n      this.addOrEdit = false\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}